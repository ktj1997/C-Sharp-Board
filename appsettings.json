{
  "ConnectionStrings": {
      "DefaultDbConnectionString":"Server=(localdb)\\mssqllocaldb;Database=csharp;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "Serilog":{
    "MinimumLevel":{
      "Default": "Information",
      "Override":{
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo":[
      {
        "Name": "File",
        "Args":{
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args":{
          "serverUrl": "http://serilog_seq:5341"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
